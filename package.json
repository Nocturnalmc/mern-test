{
  "name": "mern-test",
  "version": "1.0.0",
  "description": "testing mern stack how it works",
  "main": "server.js",
  "scripts": {
    "install-server": "npm install",
    "install-client": "cd 5-2-2024 && npm install",
    "install-prod": "npm ci && cd 5-2-2024 && npm ci",
    "prune-prod": "npm prune --omit=dev && cd 5-2-2024 && npm prune --omit=dev",
    "server": "nodemon server.js",
    "client": "cd 5-2-2024 && npm run dev",
    "dev": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\"",
    "build-client": "cd 5-2-2024 && npm run build",
    "deploy-local": "npm run build-client && npm start",
    "start": "node server.js"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
