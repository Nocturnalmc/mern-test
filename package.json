{
  "name": "mern-test",
  "version": "1.0.0",
  "description": "testing mern stack how it works",
  "main": "server.js",
  "scripts": {
    "install-server": "npm install",
    "install-client": "cd 22-3-2023 && npm install",
    "server": "nodemon server.js",
    "client": "cd 22-3-2023 && npm run dev",
    "dev": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\"",
    "build-client": "cd 22-3-2023 && npm run build",
    "deploy-local": "npm run build-client && npm start",
    "start": "node server.js"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "dotenv": "^16.0.2",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15"
  }
}
